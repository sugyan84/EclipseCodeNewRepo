<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
			
<beans>
	<bean id="car" class="beans.Car" scope="prototype">
		<property name="carName" value="Audi"></property>
		<property name="e" ref ="engine"></property>
	</bean>


	<bean id="engine" class="beans.Engine" scope="prototype">
		<property name="modelno" value="2016"></property>	
	</bean>

</beans>		